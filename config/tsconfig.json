{
  "compilerOptions": {
    "rootDir": "../src",
    "outDir": "../dist",
    "module": "esnext",
    "target": "esnext",
    "moduleResolution": "node",
    "sourceMap": false,
    "allowJs": false,
    "experimentalDecorators": true,
    "noImplicitUseStrict": true,
    "removeComments": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "suppressImplicitAnyIndexErrors": false,
    "noUnusedLocals": true,
    "esModuleInterop": true,
    "lib": [
      "es2017",
      "dom",
      "es5"
    ],
    "typeRoots": [
      "../node_modules/@types"
    ]
  },
  "include": [
    "../src/vue-context.ts"
  ],
  "exclude": [
    "../node_modules",
    "../src/test.ts",
    "../demo",
    "../dist"
  ]
}
